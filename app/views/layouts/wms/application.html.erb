<!DOCTYPE html>
<html>
<head>
  <title>Wms</title>
  <%= stylesheet_link_tag    "wms/application", media: "all" %>
  <%= javascript_include_tag "wms/application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'shared/header' %>
<% if flash[:notice]%>
<div class="flash-notice">
  <%= flash[:notice] %>
</div>
<% end %>
<% if flash[:error]%>
<div class="flash-error">
  <%= flash[:error] %>
</div>
<% end %>
<%= yield %>

</body>
</html>
