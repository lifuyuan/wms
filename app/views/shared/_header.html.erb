<div class="header">
  <div class="container clearfix">
    <%= link_to "WMS", root_path, class: "header-logo-wordmark" %>
    <ul id="user-links">
      <% if current_account %>
        <li><%= link_to current_account.name, '#' %></li>
        <li><%= link_to "退出", logout_path, method: "delete" %></li>
      <% else %>
        <li><%= link_to "注册", signup_path %></li>
        <li><%= link_to "登录", login_path %></li>
      <% end %>
    </ul>
  </div>
</div>